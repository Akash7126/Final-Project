@{
}

@model Online_Exam_System.Models.Student



<form asp-controller="Student" asp-action="GetStudentDetails">

    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Student Name</th>
                <th>DepartmentId</th>
                <th>BatchId</th>
                <th>StudentPassword</th>
                <th>Contact</th>
                <th>Sex</th>
                <th>Email</th>
                <th>Address</th>
            </tr>
        </thead>
        <tr>
            <td>@Model.StudentName</td>
            <td>@Model.Department.DepartmentName</td>
            <td>@Model.Batch.BatchName</td>
            <td>@Model.StudentPassword</td>
            <td>@Model.Contact</td>
            <td>@Model.Sex</td>
            <td>@Model.Email</td>
            <td>@Model.Address</td>
        </tr>     
    </table>
</form>
